- name: slim-inception-v1
  description: Inception v1 classifier in TF-Slim
  operations:
    train:
      description: Train Inception v1
      cmd: train --model_name inception_v1
      requires:
        - slim/lib
        - datasets:${dataset}
      flags:
        $include: train-flags
    fine-tune:
      description: Fine-tune Inception v1
      cmd: train --model_name inception_v1
      requires:
        - slim/lib
        - datasets:${dataset}
        - checkpoints:${checkpoint}

