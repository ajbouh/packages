# -*-yaml-*-

name: slim-resnet-v2
extends: slim-fine-tunable
private: yes

operations:
  evaluate:
    cmd: >
      slim_op eval_image_classifier \
        --model_name {{model_name}} \
        --eval_dir . \
        --dataset_split_name validation \
        --dataset_dir data \
        --checkpoint_path model \
        --preprocessing_name inception \
        --eval_image_size 299
